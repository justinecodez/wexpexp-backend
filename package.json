{
    "name": "tanzania-event-platform-backend",
    "version": "1.0.0",
    "description": "Event Planning & Services Platform Backend for Tanzania",
    "main": "dist/app.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/app.js",
        "dev": "nodemon src/server.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:integration": "jest --config jest.integration.config.js",
        "test:integration:watch": "jest --config jest.integration.config.js --watch",
        "db:migrate": "typeorm migration:run -d src/config/database.ts",
        "db:revert": "typeorm migration:revert -d src/config/database.ts",
        "db:generate": "typeorm migration:generate -d src/config/database.ts",
        "db:create": "typeorm migration:create",
        "db:seed": "tsx src/database/seed.ts",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "logs:quiet": "node scripts/logging-control.js quiet",
        "logs:normal": "node scripts/logging-control.js normal",
        "logs:verbose": "node scripts/logging-control.js verbose",
        "logs:debug": "node scripts/logging-control.js debug",
        "logs:status": "node scripts/logging-control.js status",
        "logs:help": "node scripts/reduce-logging.js"
    },
    "keywords": [
        "event-planning",
        "tanzania",
        "express",
        "typescript",
        "typeorm"
    ],
    "author": "Tanzania Event Platform",
    "license": "MIT",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.940.0",
        "@aws-sdk/s3-request-presigner": "^3.940.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.10.5",
        "@types/nodemailer": "^6.4.14",
        "@types/xlsx": "^0.0.35",
        "axios": "^1.11.0",
        "bcryptjs": "^2.4.3",
        "bullmq": "^5.65.0",
        "cors": "^2.8.5",
        "csv-parser": "^3.0.0",
        "csv-writer": "^1.6.0",
        "date-fns": "^4.1.0",
        "dotenv": "^16.6.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "form-data": "^4.0.5",
        "helmet": "^7.1.0",
        "ioredis": "^5.8.2",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.1",
        "mssql": "^11.0.1",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.7",
        "pg": "^8.16.3",
        "qrcode": "^1.5.3",
        "react": "^19.2.3",
        "reflect-metadata": "^0.2.1",
        "socket.io": "^4.7.4",
        "sqlite3": "^5.1.7",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "typeorm": "^0.3.20",
        "winston": "^3.11.0",
        "xlsx": "^0.18.5",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/jest": "^29.5.8",
        "@types/morgan": "^1.9.10",
        "@types/qrcode": "^1.5.5",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "eslint": "^8.54.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "prettier": "^3.1.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "tsx": "^4.6.2",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
